    "di": "0.0.1",
    "fast-deep-equal": "^3.1.3",
    "objectdiff": "^1.1.0",
    "optimist": "~0.6.1",
    "q": "^1.5.1",
    "validate.js": "^0.12.0",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
